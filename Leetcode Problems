//Best time to sell and buy stock

class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int b=INT_MAX;
        int o=0;
        int s=0;

        for(int i=0; i<prices.size(); i++){
            if (prices[i]<b){
                b=prices[i];
            }
            s=prices[i]-b;
            if(o<s){
                o=s;
            } 
        }
        return o;
    }
};
